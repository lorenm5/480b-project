@interact
def __(Start = text_control("What would you like to do?"), Selections = selector(['Linear Algebra','Integrate','Differentiate',                             'Trig','Plot', 'nothing'], default='nothing', nrows=2, ncols=3,label = "Sections",buttons=False)):
    if Selections == 'Linear Algebra':
        @interact
        def __(m = input_grid(1, 2, default=3, label='Matrix size: rows, colums', width=2)):
            v = m[0]
            @interact
            def __(Enter_matrix = matrix(QQ, v[0], v[1], range(v[0] * v[1]))):
                @interact
                def __(Linear_Algebra = text_control("What would you like to do?"), solve_right =
                       checkbox(False, label='Solve right') , ker = checkbox(False, label='Kernel'), eigenvalue =
                       checkbox(False, label='Eigenvalues'), eigenvec = checkbox(False, label='Eigenvectors')):

                    if solve_right == True:
                        @interact
                        def __(Enter_vector = matrix(QQ, v[1], 1, range(v[1]))):
                            try:
                                x = Enter_matrix\Enter_vector
                                html('$$ %s %s = %s$$' %(latex(Enter_matrix), latex(x), latex(Enter_vector)))
                            except:
                                html('no solution to $$ %s x = %s$$' %(latex(Enter_matrix), latex(Enter_vector)))
                    if ker == True:
                        print kernel(Enter_matrix)
                    if eigenvalue == True:
                        print 'Eigenvalues'
                        print Enter_matrix.eigenvalues()
                    if eigenvec == True:
                        print 'Eigenvectors'
                        v = (Enter_matrix.eigenvectors_left())

                        while i in range(len(v)):
                            print v[i]
    elif Selections == 'Integrate':
        @interact
        def __(a=input_box(e^x + x^2*log(x) + 5*cos(pi*x), 'What would you like to integrate?')):
            b = a.integrate(x)
            html('$$ \int (%s) dx = %s$$' %(latex(a), latex(b)))
            @interact
            def __(Integral = text_control("Would you like to see a plot?"), plotEq = checkbox(False, label='Equation plot') , inter_Eq                             = checkbox(False, label='Integral plot')):
                if plotEq == True and inter_Eq == True:
                    Pa = plot(a)
