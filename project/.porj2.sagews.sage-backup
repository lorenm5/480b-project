    elif Selections == 'Differentiate':
        @interact
        def __(a=input_box(e^x + x^2*log(x) + 5*cos(pi*x), 'What would you like to differetiate?')):
            b = a.diff(x)
            html('$$ d/dx (%s) = %s$$' %(latex(a), latex(b)))
            @interact
            def __(Integral = text_control("Would you like to see a plot?"), plotEq = checkbox(False, label='Equation plot') , inter_Eq                             = checkbox(False, label='Derivitive plot')):
                if plotEq == True and inter_Eq == True:
